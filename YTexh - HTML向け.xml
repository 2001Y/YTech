    <main>
      <b:section id='main-content'>
        <b:widget id='Blog1' type='Blog' locked='true'>
          <b:includable id='main'>
            <b:if cond='data:blog.pageType in {"item" , "static_page"}'>
              <b:loop values='data:posts' var='post'>
                <div class="post-main">
                  <h1><data:post.title/></h1>
                  <div class="post-meta">
                    <span class="post-day"><data:post.dateHeader/></span>
                    <span class="post-time"><data:post.timestamp/></span>
                    <ul class="post-category">
                      <b:loop values='data:post.labels' var='label'>
                        <li>
                          <a expr:href='data:label.url'><data:label.name/></a>
                        </li>
                      </b:loop>
                    </ul>
                  </div>
                  <div class="post-body">
                    <data:post.body/>
                  </div>
                  <div id="disqus">
                    <div id="disqus_thread"></div>
                    <noscript>
                      JavaScriptが無効になっているためコメントできません。
                    </noscript>
                  </div>
                </div>
                <div class="copy delay">
                  コピー完了
                </div>
                <div class='light-box delay'>
                  <img src=''/>
                </div>
              </b:loop>
              <b:elseif cond='data:blog.pageType == "error_page"'/>
              <h1>404</h1>
              <p>
                ページが存在しません。
              </p>
              <b:else/>
              <b:loop values='data:posts' var='post'>
                <a expr:href='data:post.canonicalUrl' class="post-link">
                  <div class="post-list">
                    <div class="post-tmb">
                      <b:if cond='data:post.firstImageUrl'>
                        <b:with value='resizeImage(data:post.firstImageUrl,1000)' var="tmb-img">
                          <img class="post-tmb-img" expr:src="data:tmb-img"/>
                        </b:with>
                        <b:else/>
                        <img
                          class="post-tmb-img"
                          src='data:image/jpeg;base64,'/>
                      </b:if>
                      <div class="post-tmb-content">
                        <h2><data:post.title/></h2>
                        <div class="post-meta">
                          <b:if cond='data:post.dateHeader'>
                            <span class="post-day"><data:post.dateHeader/></span>
                            <span class="post-time">
                              <data:post.timestamp/>
                                <script>
                                  var postday = "<data:post.dateHeader/>";
                                </script>
                            </span>
                            <b:else/>
                            <script>
                              document.write(postday)
                            </script>
                            <data:post.timestamp/>
                          </b:if>
                          <ul class="post-category">
                            <b:loop values='data:post.labels' var='label'>
                              <li>
                                <data:label.name/>
                              </li>
                            </b:loop>
                          </ul>
                        </div>
                      </div>
                    </div>
                  </div>
                </a>
              </b:loop>
            </b:if>
            <div class="page-nav">
              <b:if cond='data:newerPageUrl'>
